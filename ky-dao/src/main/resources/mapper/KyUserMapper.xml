<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ky.dao.mapper.KyUserMapper">

    <resultMap id="BaseResultMap" type="com.ky.dao.entity.KyUserDO">
            <id property="userId" column="user_id" jdbcType="INTEGER"/>
            <result property="userName" column="user_name" jdbcType="VARCHAR"/>
            <result property="contact" column="contact" jdbcType="VARCHAR"/>
            <result property="contactType" column="contact_type" jdbcType="TINYINT"/>
            <result property="address" column="address" jdbcType="VARCHAR"/>
            <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
            <result property="updateTime" column="update_time" jdbcType="TIMESTAMP"/>
            <result property="valid" column="valid" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        user_id,user_name,contact,contact_type,address,create_time,update_time,valid
    </sql>
    <insert id="insertBatch">
        insert into ky_user (user_name,contact,contact_type,address,create_time,update_time,valid) values
        <foreach collection="userList" item="item" separator=",">
            (#{item.userName},#{item.contact},#{item.contactType},#{item.address},#{item.createTime},#{item.updateTime},#{item.valid})
        </foreach>
    </insert>
</mapper>
